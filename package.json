{
  "name": "@palmabit/react-cookie-law",
  "version": "0.6.2",
  "description": "React Cookie banner GDPR compliance",
  "author": "Alessandro Sarzina <a.sarzina@palmabit.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Palmabit-IT/react-cookie-law.git"
  },
  "bugs": {
    "url": "https://github.com/Palmabit-IT/react-cookie-law/issues"
  },
  "homepage": "https://github.com/Palmabit-IT/react-cookie-law#readme",
  "keywords": [
    "gdpr",
    "cookie",
    "cookie law",
    "banner",
    "cookie banner",
    "react cookie banner",
    "react"
  ],
  "main": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "test": "jest --coverage",
    "precommit": "lint-staged",
    "prepublish": "npm test && npm run build",
    "lint": "eslint 'src/**/*.js'",
    "testCI": "npm run test && cat coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-transform-modules-commonjs": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint 'src/**/*.js' --fix",
      "git add --all ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
